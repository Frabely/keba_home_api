# Required
KEBA_IP=192.168.1.10

# Optional (defaults are used when omitted)
# KEBA_SOURCE: udp (default) | modbus
KEBA_SOURCE=udp
KEBA_UDP_PORT=7090
KEBA_MODBUS_PORT=502
KEBA_MODBUS_UNIT_ID=255
KEBA_MODBUS_ENERGY_FACTOR_WH=0.1
POLL_INTERVAL_MS=1000
DB_PATH=/var/lib/keba/keba.db
HTTP_BIND=0.0.0.0:8080
DEBOUNCE_SAMPLES=2

# Optional logging filter
RUST_LOG=info
